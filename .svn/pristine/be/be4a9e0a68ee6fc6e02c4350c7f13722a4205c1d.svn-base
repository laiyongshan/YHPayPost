    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/bg_white"
                android:divider="@drawable/divider_horizontal"
                android:dividerPadding="5dp"
                android:orientation="vertical"
                android:showDividers="middle">

                <LinearLayout
                    android:id="@+id/fmperson_verify_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/fee" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="实名认证" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_psw_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_password" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="修改密码" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_contract_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_phone" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="联系我们" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_about_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_about" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="关于我们" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_update_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_update" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="检查更新" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_help_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_help" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="帮助中心" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


                <LinearLayout
                    android:id="@+id/fmperson_share_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_downland" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="下载分享" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/fmperson_planCard_ll"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:foreground="@drawable/selector_foreground"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:visibility="visible">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_downland" />

                    <TextView
                        style="@style/text_normal_color_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dp"
                        android:layout_weight="1"
                        android:text="开通规划" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@mipmap/icon_next" />
                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:id="@+id/fmperson_exit_ll"
                android:layout_width="match_parent"
                android:layout_height="45dp"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="10dp"
                android:background="@color/bg_white"
                android:foreground="@drawable/selector_foreground"
                android:gravity="center">


                <TextView
                    style="@style/text_theme_16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="注销登录" />
            </LinearLayout>


        </LinearLayout>
    </android.support.v4.widget.NestedScrollView>


</LinearLayout>
