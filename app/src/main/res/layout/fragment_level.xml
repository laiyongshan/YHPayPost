                            android:layout_height="wrap_content"
                            android:paddingBottom="5dp"
                            android:text="20W"
                            android:textColor="@color/text_theme" />
                    </LinearLayout>

                </LinearLayout>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
